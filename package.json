{
    "name": "igsscript",
    "displayName": "igsscript",
    "description": "Language extension for igsscript scripting language",
    "repository": "https://github.com/Mathsoum/igsscript-vscode-extension",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "igsscript",
            "aliases": ["igsscript", "igsscript"],
            "extensions": [".igsscript"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "igsscript",
            "scopeName": "source.igsscript",
            "path": "./syntaxes/igsscript.tmLanguage.json"
        }]
    }
}
